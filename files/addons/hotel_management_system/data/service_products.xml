<odoo>
    <data noupdate="1">
        <!-- Service Product -->
        <record id="product_hotel_service" model="product.product">
            <field name="name">Service</field>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="taxes_id" eval="[(6, 0, [])]" />
        </record>

        <!-- Agent Bill Product -->
        <record id="product_agent_bill" model="product.product">
            <field name="name">Agent Bill</field>
            <field name="type">service</field>
            <field name="list_price">0.0</field>
            <field name="taxes_id" eval="[(6, 0, [])]" />
        </record>

        <record id="menu_hotels" model="website.menu">
            <field name="name">Hotel</field>
            <field name="url">/hotels</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">30</field>
        </record>
    </data>
</odoo>
